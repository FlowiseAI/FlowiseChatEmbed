{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Flowise Embed",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/.bin/rollup",
      "args": [
        "--watch",
        "--config",
        "rollup.config.js"
      ],
      "env": {
        "NODE_ENV": "debug"
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "name": "Debug in Browser",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:51914",
      "webRoot": "${workspaceFolder}",
      "sourceMapPathOverrides": {
        "webpack:///./src/*": "${webRoot}/src/*",
        "webpack:///./*": "${webRoot}/*",
        "webpack:///src/*": "${webRoot}/src/*"
      },
      "smartStep": true,
      "skipFiles": [
        "<node_internals>/**",
        "node_modules/**"
      ]
    }
  ]
}